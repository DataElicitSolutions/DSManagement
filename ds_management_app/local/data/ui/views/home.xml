<dashboard version="1.1" script="dsm_home_page.js">
  <label>Deployment Home</label>
  <row>
    <panel>
      <html>
        <b>Repository Location: </b> $SPLUNK_HOME/etc/apps/ds_management_app/data/ds_app
      </html>
    </panel>
  </row>
  <row>
    <panel>
      <html>
        <div id="list_serverclass"> </div>
      </html>
    </panel>
  </row>
  <row>
    <panel>
      <title>Apps</title>
      <table>
        <search>
          <query>| inputlookup serverclass.csv 
| dedup App
| table App
| search App!="-"</query>
          <earliest>-24h@h</earliest>
          <latest>now</latest>
        </search>
        <option name="drilldown">none</option>
        <option name="refresh.display">progressbar</option>
      </table>
    </panel>
    <panel>
      <title>Server Classes</title>
      <table>
        <search>
          <query>| inputlookup serverclass.csv 
| dedup Serverclass
| table Serverclass
         </query>
          <earliest>-24h@h</earliest>
          <latest>now</latest>
        </search>
        <option name="drilldown">none</option>
        <option name="refresh.display">progressbar</option>
      </table>
    </panel>
  </row>
</dashboard>